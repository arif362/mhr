<% if params[:no_admin].present? %>
$('#global-modal').modal({backdrop: 'static', keyboard: false});
$('#global-modal').modal('show').find('.modal-content').html('<%= escape_javascript( render 'show_application_no_admin')%>')
<% else %>
$('#global-modal').modal({backdrop: 'static', keyboard: false});
$('#global-modal').modal('show').find('.modal-content').html('<%= escape_javascript( render 'show_application')%>')
<% end %>


