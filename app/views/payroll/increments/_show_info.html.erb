<div class="modal-body clearfix">
  <div class="">
    <table class="table table-user-information table-stripped">
      <tbody>
      <tr>
        <td> <%= label_tag 'Employee' %> </td>
        <td> <%= @increment.employee.full_name %> </td>
      </tr>
      <tr>
        <td> <%= label_tag 'Incremented By' %> </td>
        <td> <%= @increment.incremented_by %> </td>
      </tr>
      <tr>
        <td> <%= label_tag 'Issue Date' %> </td>
        <td> <%= @increment.created_at.strftime("%Y-%m-%d") %> </td>
      </tr>
      <tr>
        <td> <%= label_tag 'Activate Date' %> </td>
        <td> <%= format_date(@increment.active_date) %> </td>
      </tr>
      <tr>
        <td> <%= label_tag 'Status' %> </td>
        <td> <%= @increment.is_active ? "Active" : "Inactive" %> </td>
      </tr>
      </tbody>
    </table>
  </div>
</div>
<div class="modal-footer">
  <div class="btn btn-default" data-dismiss='modal'>Cancel</div>
</div>